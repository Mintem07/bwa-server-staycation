<div class="tab-pane fade show active shadow mb-4" id="show-image" role="tabpanel" aria-labelledby="home-tab">
  <div class="card-header py-3">
    <a href="/admin/item" class="btn btn-warning">Back</a>
  </div>
  <!-- DataTales Example -->
  <div class="row mx-2">
    <% for(let x = 0; x < item.imageId.length; x++) { %>
      <div class="col-md-6 my-2">
        <div class="card mb-4" style="border-top: 0;">
          <div class="card-body">
            <img src="https://admin-staycation.herokuapp.com/<%= item.imageId[x].imageUrl %>" alt="image" style="width: 100%; display: block; ">
          </div>
        </div>
      </div>
    <% } %>
  </div>
</div>